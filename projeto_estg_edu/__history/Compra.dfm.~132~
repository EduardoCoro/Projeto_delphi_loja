  object QCompra: TADOQuery
    Connection = TDMPrincipal.ADOConexao
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      
        'SELECT P.COD_COMPRA, P.COD_CLIENTE , C.NOME AS CLIENTE, P.DATA A' +
        'S DATA , P.VALOR_TOTAL'
      'FROM COMPRA AS P'
      'INNER JOIN CLIENTE AS C ON C.COD_CLIENTE = P.COD_CLIENTE'
      '')
    Left = 144
    Top = 104
  end
