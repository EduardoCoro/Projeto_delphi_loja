  object QProduto: TADOQuery
    Connection = TDMPrincipal.ADOConexao
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      
        'SELECT I.COD_PRODUTO, P.DESCRICAO, P.PRECO, I.QUANTIDADE, P.PREC' +
        'O * I.QUANTIDADE AS VALOR'
      'FROM ITENS_COMPRA I'
      'INNER JOIN COMPRA AS C ON C.COD_COMPRA = I.COD_COMPRA'
      'INNER JOIN PRODUTO AS P ON P.COD_PRODUTO = I.COD_PRODUTO')
    Left = 192
    Top = 384
  end
