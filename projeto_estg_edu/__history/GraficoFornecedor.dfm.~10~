  object QFornecedor: TADOQuery
    Active = True
    Connection = TDMPrincipal.ADOConexao
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      
        'SELECT COUNT(P.COD_PRODUTO) AS QUANTIDADE, P.COD_FORNECEDOR, F.N' +
        'OME'
      'FROM PRODUTO P'
      'INNER JOIN FORNECEDOR F ON P.COD_FORNECEDOR = F.COD_FORNECEDOR'
      'WHERE P.ATIVO = 1'
      'Group by P.COD_FORNECEDOR,F.NOME ')
    Left = 672
    Top = 40
  end
